{%- comment -%}theme-check-disable SchemaJsonFormat,LiquidTag{%- endcomment -%}
{%- if request.design_mode -%}
  {{ 'component-search.css' | asset_url | stylesheet_tag }}
  {{ 'component-menu-drawer.css' | asset_url | stylesheet_tag }}
  {{ 'component-cart-notification.css' | asset_url | stylesheet_tag }}
  {{ 'component-cart-items.css' | asset_url | stylesheet_tag }}
{%- else -%}
  <link rel="stylesheet" href="{{ 'component-search.css' | asset_url }}" media="print" onload="this.media='all'">
  <link rel="stylesheet" href="{{ 'component-menu-drawer.css' | asset_url }}" media="print" onload="this.media='all'">
  <link rel="stylesheet" href="{{ 'component-cart-notification.css' | asset_url }}" media="print" onload="this.media='all'">
  <link rel="stylesheet" href="{{ 'component-cart-items.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

{%- liquid
  assign mega_menu_link_labels = section.settings.mega_menu_link_label
  for block in section.blocks
    if block.settings.title != blank
      assign mega_menu_link_labels = mega_menu_link_labels | append: ',' | append: block.settings.title
    endif
  endfor

  assign logo_position = section.settings.logo_position
  if section.settings.search == 'bar'
    assign logo_position = 'top-left'
  endif
  assign center_menu_links = section.settings.center_menu_links
  assign localization_visible_desktop = false
  assign drawer_on_desktop = section.settings.drawer_on_desktop

  if section.settings.sticky_header_type == 'on-scroll-up' and drawer_on_desktop == 'replace_on_scroll'
    assign drawer_on_desktop = 'on'
  endif

  if logo_position == 'middle-left' and drawer_on_desktop == 'replace_on_scroll'
    assign drawer_on_desktop = 'off'
  endif
-%}

{%- if mega_menu_link_labels != blank -%}
  <link rel="stylesheet" href="{{ 'component-mega-menu.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}


{%- if settings.cart_drawer_enabled -%}
  {%- if request.design_mode -%}
    {{ 'component-cart-drawer.css' | asset_url | stylesheet_tag }}
    {{ 'component-cart.css' | asset_url | stylesheet_tag }}
    {{ 'component-totals.css' | asset_url | stylesheet_tag }}
    {{ 'component-discounts.css' | asset_url | stylesheet_tag }}
  {%- else -%}
    <link rel="stylesheet" href="{{ 'component-cart-drawer.css' | asset_url }}" media="print" onload="this.media='all'">
    <link rel="stylesheet" href="{{ 'component-cart.css' | asset_url }}" media="print" onload="this.media='all'">
    <link rel="stylesheet" href="{{ 'component-totals.css' | asset_url }}" media="print" onload="this.media='all'">
    <link rel="stylesheet" href="{{ 'component-discounts.css' | asset_url }}" media="print" onload="this.media='all'">
   {%- endif -%}
{%- endif -%}

{%- if settings.predictive_search_enabled or settings.cart_drawer_enabled -%}
  <link rel="stylesheet" href="{{ 'component-price.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

{% style %}
  header-drawer {
    --header-drawer-font-size: {{ section.settings.drawer_text_size }}rem;
    justify-self: start;
    margin-inline-start: -1rem;
  }

  .header {
    padding: {{ section.settings.spacing | times: 0.5 | round: 0 }}rem var(--px-mobile);
  }

  .section-header {
    position: sticky; /* This is for fixing a Safari z-index issue. PR #2147 */
  }

  {%- unless drawer_on_desktop != 'off' -%}
    @media screen and (min-width: 990px) {
      header-drawer {
        display: none;
      }
    }
  {%- endunless -%}

  .menu-drawer-container {
    display: flex;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }

  .list-menu--merged .header__menu-item {
    padding-top: 0;
    padding-bottom: 1.4rem;
  }

  .list-menu--merged .header__menu-item .icon-caret {
    top: calc(50% - .9em);
  }

  summary.list-menu__item {
    padding-inline-end: 2.7rem;
  }

  .list-menu__item {
    display: inline-flex;
    line-height: calc(1 + 0.6 / var(--font-body-scale));
    gap: 0.6em;
    width: 100%;
  }

  .list-menu__item.stickerified {
    display: block;
  }

  .menu-drawer__menu-item {
    display: flex;
    gap: 0.6em;
  }

  .menu-drawer__menu-item .icon {
    width: calc(var(--header-drawer-font-size, 1.8rem) * 1.3);
    height: calc(var(--header-drawer-font-size, 1.8rem) * 1.3);
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: calc(1 + 0.8 / var(--font-body-scale));
  }

  @media screen and (max-width: 749.98px) {
    .header__heading-logo {
        width: {{ section.settings.mobile_logo_width }}px;
    }
  }

  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }

  @media screen and (min-width: 990px) {
    .header {
      padding: {{ section.settings.spacing }}rem 3rem;
    }
  }
{% endstyle %}

<lazy-script data-watch-element="details-modal" src="{{ 'details-modal.js' | asset_url }}" defer="defer"></lazy-script>
{%- if settings.cart_drawer_enabled -%}
  <script src="{{ 'cart-drawer.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

{%- if section.settings.search != 'off' -%}
  <svg xmlns="http://www.w3.org/2000/svg" class="hidden">
    <symbol id="icon-search" viewBox="0 0 512 512" fill="none">
      <path d="M221.09 64a157.09 157.09 0 10157.09 157.09A157.1 157.1 0 00221.09 64z" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="32"/>
      <path fill="none" stroke="currentColor" stroke-linecap="round" stroke-miterlimit="10" stroke-width="32" d="M338.29 338.29L448 448"/>
    </symbol>

    <symbol id="icon-close" class="icon icon-close" viewBox="0 0 512 512">
      <path fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="32" d="M368 368L144 144M368 144L144 368"/>
    </symbol>
  </svg>
{%- endif -%}

<{% if section.settings.sticky_header_type != 'none' %}sticky-header data-sticky-type="{{ section.settings.sticky_header_type }}"{% else %}div{% endif %} class="header-wrapper color-{{ section.settings.color_scheme }} gradient{% if logo_position == 'middle-left' or section.settings.menu == blank %} {{ section.settings.line_separator }}{% else %} {{ section.settings.line_separator | replace: 'border', 'md-down:border' }}{% endif %}">
  {%- liquid
    assign social_icons = true
    if settings.social_facebook_link == blank and settings.social_instagram_link == blank and settings.social_youtube_link == blank and settings.social_tiktok_link == blank and settings.social_x_link == blank and settings.social_pinterest_link == blank and settings.social_snapchat_link == blank and settings.social_tumblr_link == blank and settings.social_vimeo_link == blank and settings.social_threads_link == blank and settings.social_linkedin_link == blank and settings.social_whatsapp_link == blank and settings.social_custom_icon == blank
      assign social_icons = false
    endif

    assign localization_forms = false
    if localization.available_countries.size > 1 and section.settings.enable_country_selector or section.settings.enable_language_selector and localization.available_languages.size > 1
      if localization_visible_desktop
        assign localization_forms = true
      endif
    endif
  -%}

  {%- capture inline_nav -%}
    <custom-nav>
      <ul class="list-menu list-menu--inline{% if section.settings.menu_color_scheme.settings.background == section.settings.color_scheme.settings.background %}{% unless logo_position == 'middle-left' or section.settings.line_separator contains 'border-top' %} list-menu--merged{% endunless %}{% endif %} {% if logo_position contains "left" and center_menu_links %} lg:justify-center{% endif %}" role="list">
        {%- liquid
          assign mega_menu_labels = mega_menu_link_labels | downcase | replace: ',,', ',' | replace: ' ,', ',' | replace: ', ', ',' | split: ','
        -%}
        {%- for link in section.settings.menu.links -%}
          {%- liquid
            assign link_title = link.title | downcase
            assign is_mega_menu = false
            if mega_menu_labels contains link_title
              assign is_mega_menu = true
            endif
          -%}
          <li>
            {%- if link.links != blank or is_mega_menu -%}
              <header-menu class="block">
                <details id="Details-HeaderMenu-{{ forloop.index }}"{% if is_mega_menu %} class="mega-menu"{% endif %}>
                  <summary
                      id="HeaderMenu-{{ link.handle }}"
                      class="header__menu-item list-menu__item link focus-inset"
                  >
                    <{% if section.settings.mega_menu_link_enabled %}a href="{{ link.url }}"{% else %}span{% endif %} class="header__summary-link{% if link.child_active %} header__active-menu-item{% endif %} {{ section.settings.text_size }} font-{{ section.settings.text_weight }}">
                      {{- link.title | escape -}}
                    </{% if section.settings.mega_menu_link_enabled %}a{% else %}span{% endif %}>
                    {% render 'icon-caret' %}
                  </summary>
                  {%- liquid
                    if is_mega_menu
                      render 'header-mega-menu', link: link, forloop_index: forloop.index, promos: section.blocks, text_size: section.settings.text_size
                    else
                      render 'header-dropdown-menu', link: link, forloop_index: forloop.index, promos: section.blocks, text_size: section.settings.text_size
                    endif
                  -%}
                </details>
              </header-menu>
            {%- else -%}
              <a
                  id="HeaderMenu-{{ link.handle }}"
                  href="{{ link.url }}"
                  class="header__menu-item list-menu__item link link--text focus-inset {{ section.settings.text_size }} font-{{ section.settings.text_weight }}"
                  {% if link.current %}
                    aria-current="page"
                  {% endif %}
              >
              <span
                {%- if link.current %}
                  class="header__active-menu-item"
                {% endif %}
              >
                {{- link.title | escape -}}
              </span>
              </a>
            {%- endif -%}
          </li>
        {%- endfor -%}
      </ul>
    </custom-nav>
  {%- endcapture -%}

  <header class="header header--{{ logo_position }} header--mobile-{{ section.settings.mobile_logo_position }}{% if section.settings.let_it_snow %} let-it-snow-let-it-snow-let-it-snow{% endif %} page-width{% if drawer_on_desktop != 'off' %} drawer-menu{% if drawer_on_desktop == 'replace_on_scroll' %} drawer-menu--on-scroll{% endif %}{% endif %}{% if section.settings.menu != blank %} header--has-menu{% endif %}{% if social_icons %} header--has-social{% endif %}{% if shop.customer_accounts_enabled and section.settings.enable_accounts or section.settings.enable_accounts_mobile %} header--has-account{% endif %}{% if localization_forms %} header--has-localizations{% endif %}{% if section.settings.search != 'off' and section.settings.search_mobile_open %} header--keep-mobile-search{% endif %}">
    {%- liquid
      if section.settings.menu_burger != blank or section.settings.menu != blank
        render 'header-drawer', promos: section.blocks
      endif

      if section.settings.search != 'off' and logo_position == 'top-center'
        render 'header-search', input_id: 'Search-In-Modal-1'
      endif
    -%}

    {%- if logo_position != 'middle-center' -%}
      <a href="{{ routes.root_url }}" class="header__heading-link link link--text focus-inset">
        {%- if section.settings.logo != blank -%}
          <div class="header__heading-logo-wrapper">
            {%- assign logo_alt = section.settings.logo.alt | default: shop.name | escape -%}
            {%- assign logo_height = section.settings.logo_width | divided_by: section.settings.logo.aspect_ratio -%}
            {% capture sizes %}(max-width: {{ section.settings.logo_width | times: 2 }}px) 50vw, {{ section.settings.logo_width }}px{% endcapture %}
            {% capture widths %}{{ section.settings.logo_width }}, {{ section.settings.logo_width | times: 1.5 | round }}, {{ section.settings.logo_width | times: 2 }}{% endcapture %}
            {{ section.settings.logo | image_url: width: 600 | image_tag:
              class: 'header__heading-logo motion-reduce',
              widths: widths,
              height: logo_height,
              width: section.settings.logo_width,
              alt: logo_alt,
              sizes: sizes,
              preload: true
            }}
          </div>
        {%- else -%}
          <span class="h4">{{ shop.name }}</span>
        {%- endif -%}
        {%- if section.settings.logo_strap != blank -%}
          <p class="header__strap visible-md rte opacity-90 {{ section.settings.logo_strap_text_size }}">
            {%- if section.settings.logo_strap contains '<a ' -%}
              {{ section.settings.logo_strap | strip_html }}
            {%- else -%}
              {{ section.settings.logo_strap }}
            {%- endif -%}
          </p>
        {%- endif -%}
      </a>
    {%- endif -%}

    {%- if section.settings.search == 'bar' or section.settings.search_mobile_open and section.settings.search != 'off' -%}
      <div class="header__search-bar{% if section.settings.search == 'bar' and section.settings.search_mobile_open == false %} small-hide medium-hide{% elsif section.settings.search != 'bar' and section.settings.search_mobile_open %} hidden-md{% endif %} flex justify-end">
        {%- render 'header-search-box', input_id: 'Inline-Search' -%}
      </div>
    {%- endif -%}

    {%- if section.settings.menu != blank and logo_position == 'middle-left' -%}
      <nav class="header__inline-menu{% if drawer_on_desktop == 'replace_on_scroll' %} header__inline-menu--hide-on-scroll{% endif %}{% if logo_position contains "left" and center_menu_links %} lg:flex lg:justify-center{% endif %}">
        {{ inline_nav }}
      </nav>
    {%- endif -%}

    <div class="header__icons{% if localization_forms %} header__icons--localization header-localization{% endif %}">
      {%- if section.settings.cta_enable -%}
        {%- if section.settings.cta_label != blank or section.settings.cta_heading != blank -%}
          {%- capture cta_content -%}
            {%- if section.settings.cta_icon != blank -%}
              {%- assign icon_size = section.settings.cta_icon_size | times: 10 -%}
              {%- render 'icon', icon: section.settings.cta_icon, size: icon_size -%}
            {%- endif -%}
            <div>
              {%- if section.settings.cta_heading -%}
                <p class="m-0 lh-13 {{ section.settings.cta_text_size }}"><strong>{{ section.settings.cta_heading }}</strong></p>
              {%- endif -%}
              <p class="m-0 lh-13 opacity-80 {{ section.settings.cta_text_size }}">{{ section.settings.cta_label }}</p>
            </div>
          {%- endcapture -%}

          {%- if section.settings.cta_url != blank -%}
            <a href="{{ section.settings.cta_url }}" class="header__cta z-1 link un-underlined-link decoration-none flex items-center border-radius-input p-1 gap-1 pl-15 pr-2 whitespace-nowrap hidden-xs medium-hide color-{{ section.settings.cta_color_scheme }} gradient">
              {{ cta_content }}
            </a>
          {%- else -%}
            <div class="header__cta flex items-center z-1 p-1 gap-1 pl-15 pr-2 whitespace-nowrap hidden-xs medium-hide border-radius-input color-{{ section.settings.cta_color_scheme }} gradient">
              {{ cta_content }}
            </div>
          {%- endif -%}
        {%- endif -%}
      {%- endif -%}

      {%- if localization_forms -%}
        {%- liquid
          if section.settings.enable_country_selector and localization.available_countries.size > 1
            assign show_country_picker = true
          endif

          if section.settings.enable_language_selector and localization.available_languages.size > 1
            assign show_language_picker = true
          endif
        -%}

        <div class="desktop-localization-wrapper pr-1">
          <localization-form class="small-hide medium-hide no-js-hidden" data-prevent-hide>
            {%- form 'localization', id: 'MenuLocalizationForm', class: 'localization-form' -%}
              <div>
                {%- render 'localization',
                    localPosition: 'HeaderLocalization',
                    show_country_picker: show_country_picker,
                    show_language_picker: show_language_picker,
                    show_country_name: false,
                    show_country_filter: section.settings.show_country_filter,
                    always_show_currency: true,
                    disclosure_class: 'disclosure--has-arrow',
                    abbreviate: false
                -%}
              </div>
            {%- endform -%}
          </localization-form>
        </div>
      {%- endif -%}

      {%- if section.settings.search != 'off' -%}
        {%- unless section.settings.search == 'bar' and section.settings.search_mobile_open -%}
          <div class="{% if section.settings.search == 'bar' %}large-up-hide{% endif %}{% if section.settings.search_mobile_open %} visible-md{% endif %}">
            {% render 'header-search', input_id: 'Search-In-Modal' %}
          </div>
        {%- endunless -%}
      {%- endif -%}

      {%- if shop.customer_accounts_enabled and section.settings.enable_accounts or section.settings.enable_accounts_mobile -%}
        <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" class="header__icon header__icon--account link focus-inset{% if section.settings.enable_accounts and section.settings.enable_accounts_mobile == false %} small-hide{% elsif section.settings.enable_accounts == false and section.settings.enable_accounts_mobile %} visible-xs{% endif %}">
          <account-icon>
            {%- if customer and customer.has_avatar? -%}
              {{ customer | avatar }}
            {%- else -%}
              {% render 'icon-account' %}
            {%- endif -%}
          </account-icon>
          <span class="visually-hidden">
            {%- liquid
              if customer
                echo 'customer.account_fallback' | t
              else
                echo 'customer.log_in' | t
              endif
            -%}
          </span>
        </a>
      {%- endif -%}

      {%- if section.settings.custom_icon != blank -%}
        <a href="{{ section.settings.custom_icon_url }}" class="header__icon header__icon--custom link focus-inset"
            {% if section.settings.custom_icon_label %} title={{ section.settings.custom_icon_label | json }}{% endif %}
        >
          {%- render 'icon', icon: section.settings.custom_icon -%}
        </a>
      {%- endif -%}

      <a href="{{ routes.cart_url }}" class="header__icon header__icon--cart link focus-inset" id="cart-icon-bubble" data-no-instant>
        {%- liquid
          if settings.cart_icon == 'cart'
            render 'icon-cart-cart'
          elsif settings.cart_icon == 'bag'
            render 'icon-cart-bag'
          elsif settings.cart_icon == 'basket'
            render 'icon-cart-basket'
          endif
        -%}
        <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
        {%- if cart != empty -%}
          <div class="cart-count-bubble z-1 font-bold">
            <span aria-hidden="true"{% if cart.item_count > 999 %} style="font-size: 8px; white-space: nowrap;"{% endif %}>{{ cart.item_count }}</span>
            <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
          </div>
        {%- endif -%}
      </a>
    </div>
  </header>

  {%- if section.settings.menu != blank and logo_position == 'top-left' -%}
    <nav class="header__inline-menu{% if drawer_on_desktop == 'replace_on_scroll' %} header__inline-menu--hide-on-scroll{% endif %} color-{{ section.settings.menu_color_scheme }} gradient {{ section.settings.line_separator }}{% if logo_position contains "left" and center_menu_links %} lg:flex lg:justify-center{% endif %}">
      <div class="page-width{% if section.settings.color_scheme == section.settings.menu_color_scheme and section.settings.line_separator == 'border-bottom' or section.settings.line_separator == blank %} pb-05{% endif %}">
      {{ inline_nav }}
      </div>
    </nav>
  {%- endif -%}
</{% if section.settings.sticky_header_type != 'none' %}sticky-header{% else %}div{% endif %}>

{%- if settings.atc_action == "notification" or settings.atc_action == "drawer" and settings.cart_drawer_enabled == false -%}
  {%- render 'cart-notification', drawer_on_desktop: drawer_on_desktop -%}
{%- endif -%}

{% javascript %}
  class StickyHeader extends HTMLElement {
    constructor() {
      super();
    }

    connectedCallback() {
      this.header = document.querySelector('.section-header');
      this.headerIsAlwaysSticky = this.getAttribute('data-sticky-type') === 'always';
      this.headerBounds = {};

      this.headerHeight = this.header.offsetHeight;
      document.documentElement.style.setProperty('--header-height', `${this.headerHeight}px`);
      document.documentElement.style.setProperty('--header-height-live', `${this.headerHeight}px`);

      window.addEventListener('resizeend', () => {
        // Only update height once per resize cycle, off the main thread
        requestAnimationFrame(() => {
          this.headerHeight = this.header.offsetHeight;
          document.documentElement.style.setProperty('--header-height', `${this.headerHeight}px`);
          document.documentElement.style.setProperty('--header-height-live', `${this.headerHeight}px`);
        });
      });

      if (this.headerIsAlwaysSticky) {
        this.header.classList.add('shopify-section-header-sticky');
      }

      this.currentScrollTop = 0;
      this.preventReveal = false;
      this.predictiveSearch = this.querySelector('predictive-search');
      this.lastScrollTrigger = 0;
      this.scrollThreshold = 50;

      this.onScrollHandler = this.onScroll.bind(this);
      this.hideHeaderOnScrollUp = () => (this.preventReveal = true);

      this.addEventListener('preventHeaderReveal', this.hideHeaderOnScrollUp);
      window.addEventListener('scroll', this.onScrollHandler, { passive: true });

      this.searchButton = this.querySelector('.header__icon--search');
      if (this.searchButton) {
        this.searchButton.addEventListener('click', this.searchButtonClick.bind(this));
      }

      this.createObserver();
    }

    searchButtonClick() {
      this.preventHide = true;
      requestAnimationFrame(() => (this.preventHide = false));
    }

    // Defer the DOM write to avoid synchronous reflow
    setHeaderHeightLive(event, zero) {
      requestAnimationFrame(() => {
        const isVisible = !this.header.classList.contains('shopify-section-header-hidden');
        const height = (zero || !isVisible) ? 0 : this.header.offsetHeight;
        document.documentElement.style.setProperty('--header-height-live', `${height}px`);
      });
    }

    disconnectedCallback() {
      this.removeEventListener('preventHeaderReveal', this.hideHeaderOnScrollUp);
      window.removeEventListener('scroll', this.onScrollHandler);
    }

    createObserver() {
      let observer = new IntersectionObserver((entries, observer) => {
        this.headerBounds = entries[0].intersectionRect;
        observer.disconnect();
      });

      observer.observe(this.header);
    }

    onScroll() {
      requestAnimationFrame(() => {
        const scrollTop = window.pageYOffset || document.documentElement.scrollTop;

        if (this.predictiveSearch && this.predictiveSearch.isOpen) return;

        // Only trigger actions if the scroll has changed by at least the threshold value
        if (Math.abs(scrollTop - this.lastScrollTrigger) < this.scrollThreshold) return;

        const scrollDifference = scrollTop - this.currentScrollTop;
        // Handle scrolling classes based on 50px sensitivity
        if (scrollDifference > 50 && this.pauseScrollListen !== true) {
          // Scrolling down
          this.pauseScrollListen = true;
          this.header.classList.add('scrolling-down');
          this.header.classList.remove('scrolling-up');
          setTimeout(() => {
            this.setHeaderHeightLive();
            this.pauseScrollListen = false;
          }, 320);
        } else if (scrollDifference < -50 && this.pauseScrollListen !== true) {
          // Scrolling up
          this.pauseScrollListen = true;
          this.header.classList.add('scrolling-up');
          this.header.classList.remove('scrolling-down');
          setTimeout(() => {
            this.setHeaderHeightLive();
            this.pauseScrollListen = false;
          }, 320);
        }

        if (scrollTop > this.currentScrollTop && scrollTop > this.headerBounds.bottom) {
          // Main logic for scrolling down
          this.header.classList.add('scrolled-past-header');
          if (this.preventHide) return;
          requestAnimationFrame(this.hide.bind(this));
        } else if (scrollTop < this.currentScrollTop && scrollTop > this.headerBounds.bottom) {
          // Main logic for scrolling up
          this.header.classList.add('scrolled-past-header');
          if (!this.preventReveal) {
            requestAnimationFrame(this.reveal.bind(this));
          } else {
            window.clearTimeout(this.isScrolling);

            this.isScrolling = setTimeout(() => {
              this.preventReveal = false;
            }, 66);

            requestAnimationFrame(this.hide.bind(this));
          }
        } else if (scrollTop <= this.headerBounds.top) {
          // Reset at the top
          this.header.classList.remove('scrolled-past-header');
          this.header.classList.remove('scrolling-down');
          this.header.classList.remove('scrolling-up');
          requestAnimationFrame(this.reset.bind(this));
          setTimeout(() => {
            this.setHeaderHeightLive();
          }, 320);
        }

        this.currentScrollTop = scrollTop;
        this.lastScrollTrigger = scrollTop; // Update the last scroll position that triggered an action
      });
    }

    hide() {
      if (this.headerIsAlwaysSticky) return;
      this.header.classList.add('shopify-section-header-hidden', 'shopify-section-header-sticky');
      this.closeMenuDisclosure();
      this.closeSearchModal();
      this.setHeaderHeightLive(null, true);
    }

    reveal() {
      if (this.headerIsAlwaysSticky) return;
      this.header.classList.add('shopify-section-header-sticky', 'animate');
      this.header.classList.remove('shopify-section-header-hidden');
      this.setHeaderHeightLive();
    }

    reset() {
      if (this.headerIsAlwaysSticky) return;
      this.header.classList.remove('shopify-section-header-hidden', 'shopify-section-header-sticky', 'animate');
    }

    closeMenuDisclosure() {
      this.disclosures = this.disclosures || this.header.querySelectorAll('header-menu');
      this.disclosures.forEach(disclosure => disclosure.close());
    }

    closeSearchModal() {
      this.searchModal = this.searchModal || this.header.querySelector('details-modal');
      this.searchModal?.close?.(false);
    }
  }

  customElements.define('sticky-header', StickyHeader);
{% endjavascript %}

