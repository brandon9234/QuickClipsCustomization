{%- comment -%}theme-check-disable SchemaJsonFormat{%- endcomment -%}

{%- if section.index < 3 or request.design_mode -%}
  {{ 'flexible-columns.css' | asset_url | stylesheet_tag }}
{%- else -%}
  <link rel="stylesheet" href="{{ 'flexible-columns.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}


{%- style -%}
  {% unless request.visual_preview_mode %}.section-content-{{ section.id }} {% endunless %}.block-grid {
    --num-blocks: {{ section.blocks.size }};
  }

  @media screen and (max-width: {% if section.settings.use_breakpoints %}{{ section.settings.small_breakpoint_custom | replace: 'px', '' }}px{% else %}549px{% endif %}) {
    {% unless request.visual_preview_mode %}.section-content-{{ section.id }} {% endunless %}.block-grid {
      --block-grid-gap: {{ section.settings.gap_small }}px !important;
    }
  }

  @media screen and (max-width: {% if section.settings.use_breakpoints %}{{ section.settings.medium_breakpoint_custom | replace: 'px', '' }}px{% else %}749px{% endif %}) {
    {% unless request.visual_preview_mode %}.section-content-{{ section.id }} {% endunless %}.block-grid {
      --block-grid-gap: {{ section.settings.gap_medium }}px;
    }
  }

  @media screen and (min-width: {% if section.settings.use_breakpoints %}{{ section.settings.medium_breakpoint_custom | replace: 'px', '' | plus: 1 }}px{% else %}750px{% endif %}) {
    {% unless request.visual_preview_mode %}.section-content-{{ section.id }} {% endunless %}.block-grid {
      --block-grid-gap: {{ section.settings.gap_desktop }}px;
    }
  }
{%- endstyle -%}

<div class="theme-block-section-container color-{{ section.settings.section_color_scheme }} gradient isolate {{ section.settings.line_separator }} {{ section.settings.visibility }} {%- render 'class-time-of-day-to-show' -%}"
  style="{% if section.settings.image != blank %}--overlay-opacity: {{ section.settings.image_overlay_opacity | divided_by: 100.0 }};{% endif %}"
>
    {%- if section.settings.image != blank -%}
    <div class="banner__media media{% if section.settings.round_corners %} border-radius-text-box{% endif %}
          {% if section.settings.image != blank %} image-overlay{% endif %}"
    >
      {%- liquid
        assign image_height = section.settings.image.width | divided_by: section.settings.image.aspect_ratio
        assign fetch_priority = 'auto'
        if section.index < 3 or request.design_mode
          assign fetch_priority = 'high'
        endif
      -%}
      {{
      section.settings.image
      | image_url: width: 3840
      | image_tag:
      width: section.settings.image.width,
      height: image_height,
      sizes: '100vw',
      widths: '375, 500, 1100, 1500, 1780, 2000, 3000, 3840',
      fetchpriority: fetch_priority
      }}
    </div>
  {%- endif -%}

  <div class="section__inner{% unless section.settings.section_max_width == 'full' %} page-width{% endunless %} {{ section.settings.top_padding }} {{ section.settings.bottom_padding }}">
    <div class="flexible-column z-1 content-container">

      {%- if section.settings.title != blank or section.settings.text != blank or view_all != blank -%}
        {%- if section.settings.view_all_text != blank -%}
          {% capture view_all %}
            <div class="section-view-all flexible-columns__view-all{% if settings.animations_reveal_on_scroll and section.index > 2 %} scroll-trigger animate--fade-in{% endif %}">
              <a
                  href="{{ section.settings.view_all_link }}"
                  class="link un-underlined-link button-with-icon button-with-icon--right {{ section.settings.view_all_text_size }}"
              >
                {{ section.settings.view_all_text }}
                {% render 'icon-arrow' %}
              </a>
            </div>
          {% endcapture %}
        {%- endif -%}

        <div class="flex justify-{{ section.settings.mobile_heading_alignment }} md:justify-{{ section.settings.desktop_heading_alignment }} max-w-{{ section.settings.section_max_width }} ml-auto mr-auto">
          {%- assign custom_class = 'pl-0 pr-0 mb-3 max-w-' | append: section.settings.section_max_width -%}
          {% render 'standard-section-heading',
            section_name: 'flexible-columns',
            section: section,
            custom_class: custom_class,
            view_all: view_all
          %}
        </div>
      {%- endif -%}

      <div class="flex justify-center md:w-full">
        <div class="w-full p-0{% if section.settings.hide_when_empty %} theme-block-container{% endif %} section-content section-content-{{ section.id }} max-w-{{ section.settings.section_max_width }}">
          <div class="flex flex-wrap block-grid">
            {% content_for 'blocks' %}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

