{%- comment -%}theme-check-disable NestedSnippet,SchemaJsonFormat{%- endcomment -%}
{%- if request.design_mode or section.settings.hide_when_empty == false or section.settings.product != blank -%}
  {%- liquid
    assign product = section.settings.product

    if section.settings.info_color_scheme != section.settings.section_color_scheme
      assign info_color_scheme = section.settings.info_color_scheme
    endif

    assign vendor_handle = product.vendor | handle
    if collections[vendor_handle].handle != vendor_handle
      assign vendor_url = product.vendor | url_for_vendor
    else
      assign vendor_url = collections[vendor_handle].url
    endif

    assign zoom_mode = section.settings.image_zoom
    assign product_form_id = 'product-form-' | append: section.id

    assign lazy_load = section.settings.lazy_load
    if section.index < 3 or section.index == nil
      assign lazy_load = false
    endif
  -%}

  {% unless lazy_load == false or request.design_mode %}<template class="deferred block lazy-template" data-observe-margin="800px">{% endunless %}

  <product-info
    class="section__inner block gradient color-{{ section.settings.section_color_scheme }} {{ section.settings.line_separator }} {{ section.settings.top_padding }} {{ section.settings.bottom_padding }} {{ section.settings.visibility }} {%- render 'class-time-of-day-to-show' -%}
      {% if settings.animations_reveal_on_scroll and section.index > 2 %} scroll-trigger animate--fade-in{% endif %}"
    data-section="{{ section.id }}"
    data-product-id="{{ product.id }}"
    data-update-url="false"
    data-url="{{ product.url }}"
    data-product-title="{{ product.title | escape }}"
  >
    {%- if section.index < 3 or request.design_mode -%}
      {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
      {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
      {{ 'component-price.css' | asset_url | stylesheet_tag }}
      {{ 'component-slider.css' | asset_url | stylesheet_tag }}
      {{ 'component-rating.css' | asset_url | stylesheet_tag }}
      {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
      {% unless product.has_only_default_variant %}
        {{ 'component-product-variant-picker.css' | asset_url | stylesheet_tag }}
        {{ 'component-swatch-input.css' | asset_url | stylesheet_tag }}
        {{ 'component-swatch.css' | asset_url | stylesheet_tag }}
      {% endunless %}
      {%- if product.quantity_price_breaks_configured? -%}
        {{ 'component-volume-pricing.css' | asset_url | stylesheet_tag }}
      {%- endif -%}
    {%- else -%}
      <link rel="stylesheet" href="{{ 'section-main-product.css' | asset_url }}" media="print" onload="this.media='all'">
      <link rel="stylesheet" href="{{ 'component-accordion.css' | asset_url }}" media="print" onload="this.media='all'">
      <link rel="stylesheet" href="{{ 'component-price.css' | asset_url }}" media="print" onload="this.media='all'">
      <link rel="stylesheet" href="{{ 'component-slider.css' | asset_url }}" media="print" onload="this.media='all'">
      <link rel="stylesheet" href="{{ 'component-rating.css' | asset_url }}" media="print" onload="this.media='all'">
      <link rel="stylesheet" href="{{ 'component-deferred-media.css' | asset_url }}" media="print" onload="this.media='all'">
      {% unless section.settings.product.has_only_default_variant %}
        <link rel="stylesheet" href="{{ 'component-product-variant-picker.css' | asset_url }}" media="print" onload="this.media='all'">
        <link rel="stylesheet" href="{{ 'component-swatch-input.css' | asset_url }}" media="print" onload="this.media='all'">
        <link rel="stylesheet" href="{{ 'component-swatch.css' | asset_url }}" media="print" onload="this.media='all'">
      {% endunless %}
      {%- if product.quantity_price_breaks_configured? -%}
        <link rel="stylesheet" href="{{ 'component-volume-pricing.css' | asset_url }}" media="print" onload="this.media='all'">
      {%- endif -%}
    {%- endif -%}

    <lazy-script src="{{ 'product-info.js' | asset_url }}" defer="defer"></lazy-script>
    <lazy-script src="{{ 'product-form.js' | asset_url }}" defer="defer"></lazy-script>

    {%- if product.quantity_price_breaks_configured? -%}
      <lazy-script src="{{ 'show-more.js' | asset_url }}" defer="defer"></lazy-script>
      <lazy-script src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></lazy-script>
    {%- endif -%}

    {% if zoom_mode == 'hover' %}
      <lazy-script id="EnableZoomOnHover-featured" src="{{ 'magnify.js' | asset_url }}" defer="defer" data-watch-element="media-gallery"></lazy-script>
    {% endif %}

    {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
    {%- if first_3d_model -%}
      {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
      <link
        id="ModelViewerStyle"
        rel="stylesheet"
        href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
        media="print"
        onload="this.media='all'"
      >
      <link
        id="ModelViewerOverride"
        rel="stylesheet"
        href="{{ 'component-model-viewer-ui.css' | asset_url }}"
        media="print"
        onload="this.media='all'"
      >
    {%- endif -%}

    {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
    <div class="page-width">
      <div class="product grid bg-scroll grid--1-col
        product--{{ section.settings.media_size }}
        product--left
        product--slider
        {% if section.settings.stack_thumbnails %}
          product--thumbnail
        {% else %}
          product--thumbnail_slider
        {% endif %}
        product--slider-desktop
        product--mobile-{% if section.settings.thumbnail_layout == 'both' or section.settings.thumbnail_layout == 'mobile' %}show{% else %}hide{% endif %}
        {% if product == blank %}grid--2-col-desktop{% else %}{% if product.media.size > 0 %}grid--2-col-desktop{% else %}product--no-media{% endif %}{% endif %}"
         data-product-handle="{{ product.handle }}"
         {% if product.vendor %}data-product-vendor="{{ product.vendor | handleize }}"{% endif %}
           data-variant-status="{% if product.selected_or_first_available_variant == nil %}unavailable{% elsif product.selected_or_first_available_variant.available %}in-stock{% elsif product.selected_or_first_available_variant %}out-of-stock{% endif %}"
      >
        <div class="grid__item product__media-wrapper">
          {%- if product == blank -%}
            <div class="product__media-item" style="--thumbnail-radius: {{ section.settings.thumbnail_radius }}px; --media-scale: {{ section.settings.product_media_scale | divided_by: 100.00 }}; --thumbnail-scale: {{ section.settings.thumbnail_scale | divided_by: 100.00 }}; --product-media-radius: {{ section.settings.product_media_radius }}px; --thumbnail-ratio-percent: {{ 1 | times: 100 }}%;">
              <div class="product-media-container placeholder border-radius-media global-media-settings gradient" style="--ratio: 1.0; --preview-ratio: 1.0;">
                {{ 'product-5' | placeholder_svg_tag: 'placeholder-svg' }}
              </div>
            </div>
          {%- else -%}
            {% render 'product-media-gallery', is_featured_product: true, product: product, variant_images: variant_images, scroll_to_active_variant: true, gallery_layout: 'slider', enable_sticky_info: section.settings.enable_sticky_info %}
          {%- endif -%}
        </div>

        <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}">
          <section
            id="ProductInfo-{{ section.id }}"
            class="product__info-container block{% if section.settings.gallery_layout == 'slider' and section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
          >
            <div class="product__info-container {% if info_color_scheme %}gradient color-{{ info_color_scheme }} product__info-padded product__info-padded-main border-radius-text-box{% if section.settings.card_shadow %} card--shadow{% endif %}{% if section.settings.info_fullwidth_mob %} info-fullwidth-mob{% endif %}{% endif %}">

            {%- assign is_in_panel = false -%}
            {%- for block in section.blocks -%}
              {%- liquid
                assign is_hidden = false
                if block.settings.stock_visibility == 'in_stock' and product.available != true
                  assign is_hidden = true
                elsif block.settings.stock_visibility == 'out_of_stock' and product.available
                  assign is_hidden = true
                endif
              -%}

              {%- if block.type == 'panel' -%}
                {%- liquid
                  if block.settings.color_scheme != section.settings.section_color_scheme
                    assign panel_color_scheme = block.settings.color_scheme
                  else
                    assign panel_color_scheme = false
                  endif

                  assign is_in_panel = true
                -%}
                </div>

                <div class="product__info-container product__info-panel{% if is_hidden == false %} {{ block.settings.visibility }}{% if block.settings.stock_visibility contains 'variant' and request.design_mode == false %} visible-{{ block.settings.stock_visibility }}{% endif %}{% if block.settings.show_at_specific_time_of_day and request.design_mode == false %}{% if block.settings.show_time_morning == false %} hidden-morning {% endif %}{% if block.settings.show_time_afternoon == false %} hidden-afternoon {% endif %}{% if block.settings.show_time_evening == false %} hidden-evening {% endif %}{% if block.settings.show_time_night == false %} hidden-night {% endif %}{% endif %}{% endif %}{% if panel_color_scheme %} gradient color-{{ panel_color_scheme }} product__info-padded border-radius-text-box{% if section.settings.card_shadow %} card--shadow{% endif %}{% endif %}"
                    {% if is_hidden and request.design_mode != true %} hidden{% endif %}
                    {% if block.settings.padding > 0 %}style="--info-padding-x:{{ block.settings.padding }}rem;--info-padding-y:{{ block.settings.padding }}rem;"{% endif %}
                >
                  {%- if block.settings.panel_heading != blank or block.settings.panel_icon != blank -%}
                    <{{ block.settings.heading_tag }} class="panel__label flex items-center gap-1 m-0 {{ block.settings.heading_style }} {{ block.settings.heading_text_size }} font-{{ block.settings.heading_text_weight }}">
                      {%- if block.settings.panel_icon != blank -%}
                        {%- render 'icon', icon: block.settings.panel_icon -%}
                      {%- endif -%}
                      {{ block.settings.panel_heading }}
                    </{{ block.settings.heading_tag }}>
                  {%- endif -%}
              {%- elsif is_hidden == false -%}
                {%- render 'product-block',
                    section: section,
                    block: block,
                    product: product,
                    is_featured: true,
                    product_form_id: product_form_id,
                    is_in_panel: is_in_panel,
                    vendor_url: vendor_url,
                    panel_color_scheme: panel_color_scheme,
                    info_color_scheme: info_color_scheme
                -%}
              {%- endif -%}

            {%- endfor -%}

            </div>
          </section>
        </div>
      </div>

      {%- if zoom_mode and zoom_mode != 'none' -%}
        {% render 'product-media-modal', product: product, variant_images: variant_images %}
      {%- endif -%}

      {%- assign popups = section.blocks | where: 'type', 'popup' -%}
      {%- if popups.size > 0 -%}
        <link rel="stylesheet" href="{{ 'component-popup.css' | asset_url }}" media="print" onload="this.media='all'">
        <link rel="stylesheet" href="{{ 'component-popup-page.css' | asset_url }}" media="print" onload="this.media='all'">
      {%- endif -%}
      {%- for block in popups -%}
        <modal-dialog id="PopupModal-{{ block.id }}" class="popup-modal popup-modal--small{% if block.settings.popup_style == 'side' %} popup-modal--side{% endif %}" {{ block.shopify_attributes }}>
          <template class="deferred">
          <div
            role="dialog"
            aria-label="{{ block.settings.text | escape }}"
            aria-modal="true"
            class="popup-modal__content min-w-0 custom-scrollbar"
            style="width: min({{ block.settings.side_popup_max_width }}px, 90vw)"
            tabindex="-1"
          >
            <button
              id="ModalClose-{{ block.id }}"
              type="button"
              class="popup-modal__toggle"
              aria-label="{{ 'accessibility.close' | t }}"
            >
              {% render 'icon-close' %}
            </button>
            <div class="popup-modal__content-info rte">
              {{ block.settings.content }}
              {{ block.settings.page.content }}
            </div>
          </div>
          </template>
        </modal-dialog>
      {%- endfor -%}

      {%- assign variant_picker_block = section.blocks | where: 'type', 'variant_picker' | first -%}
      {%- if variant_picker_block and variant_picker_block.settings.size_chart_page.content != blank -%}
        <link rel="stylesheet" href="{{ 'component-popup.css' | asset_url }}" media="print" onload="this.media='all'">
        <link rel="stylesheet" href="{{ 'component-popup-page.css' | asset_url }}" media="print" onload="this.media='all'">
        <modal-dialog id="PopupModal-{{ variant_picker_block.id }}" class="popup-modal popup-modal--small{% if variant_picker_block.settings.popup_style == 'side' %} popup-modal--side{% endif %}" {{ variant_picker_block.shopify_attributes }}>
          <template class="deferred">
          <div
              role="dialog"
              aria-label="{{ variant_picker_block.settings.size_chart_label | escape }}"
              aria-modal="true"
              class="popup-modal__content custom-scrollbar"
              tabindex="-1"
          >
            <button
                id="ModalClose-{{ variant_picker_block.id }}"
                type="button"
                class="popup-modal__toggle"
                aria-label="{{ 'accessibility.close' | t }}"
            >
              {% render 'icon-close' %}
            </button>
            <div class="popup-modal__content-info rte">
              {{ variant_picker_block.settings.size_chart_page.content }}
            </div>
          </div>
          </template>
        </modal-dialog>
      {%- endif -%}

      {%- if product.media.size > 0 -%}
        <lazy-script src="{{ 'product-modal.js' | asset_url }}" defer="defer" data-watch-element="media-gallery"></lazy-script>
        <lazy-script src="{{ 'media-gallery.js' | asset_url }}" defer="defer" data-watch-element="media-gallery"></lazy-script>
      {%- endif -%}

      {%- if first_3d_model -%}
        <script type="application/json" id="ProductJSON-{{ product.id }}">
          {{ product.media | where: 'media_type', 'model' | json }}
        </script>
        <script src="{{ 'product-model.js' | asset_url }}" defer></script>
      {%- endif -%}

      {%- if settings.seo_schema_product != 'none' -%}
        {%- if settings.seo_schema_product == 'shopify' -%}
          <script type="application/ld+json">
            {{ product | structured_data }}
          </script>
        {%- else -%}
          {%- liquid
            if product.selected_or_first_available_variant.featured_media
              assign seo_media = product.selected_or_first_available_variant.featured_media
            else
              assign seo_media = product.featured_media
            endif
          -%}

          <script type="application/ld+json">
            {
              "@context": "https://schema.org/",
              "@type": "Product",
              "name": {{ product.title | json }},
              "url": {{ request.origin | append: product.url | json }},
              {% if seo_media -%}
                "image": [
                  {{ seo_media | image_url: width: 1920 | prepend: "https:" | json }}
                ],
              {%- endif %}
              "description": {{ product.description | strip_html | json }},
              {% if product.selected_or_first_available_variant.sku != blank -%}
                "sku": {{ product.selected_or_first_available_variant.sku | json }},
              {%- endif %}
              "brand": {
                "@type": "Brand",
                "name": {{ product.vendor | json }}
              },
              {% if product.metafields.reviews.rating.value != blank %}
              "aggregateRating": {
                "@type": "AggregateRating",
                "ratingValue": {{ product.metafields.reviews.rating.value }},
                "reviewCount": {{ product.metafields.reviews.rating_count }}
              },
              {% endif %}
              "offers": [
                {%- for variant in product.variants -%}
                  {
                    "@type" : "Offer",
                    {%- if variant.sku != blank -%}
                      "sku": {{ variant.sku | json }},
                    {%- endif -%}
                    {%- if variant.barcode.size == 12 -%}
                      "gtin12": {{ variant.barcode | json }},
                    {%- endif -%}
                    {%- if variant.barcode.size == 13 -%}
                      "gtin13": {{ variant.barcode | json }},
                    {%- endif -%}
                    {%- if variant.barcode.size == 14 -%}
                      "gtin14": {{ variant.barcode | json }},
                    {%- endif -%}
                    "itemCondition" : "https://schema.org/NewCondition",
                    "priceValidUntil": "{{ 'now' | date: '%s' | plus: 31536000 | date: '%Y-%m-%d' | url_encode | replace: '+', '%20' }}",
                    "availability" : "https://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
                    "price" : {{ variant.price | divided_by: 100.00 | json }},
                    "priceCurrency" : {{ cart.currency.iso_code | json }},
                    "url" : {{ request.origin | append: variant.url | json }},
                    "seller": {
                      "@type": "Organization",
                      "name": {{ shop.name | json }},
                      {% if section.settings.logo %}
                        "logo": {{ section.settings.logo | image_url: width: 500 | prepend: "https:" | json }},
                      {% endif %}
                      "url": {{ request.origin | append: page.url | json }}
                    }
                  }{% unless forloop.last %},{% endunless %}
                {%- endfor -%}
              ]
            }
          </script>
        {%- endif -%}
      {%- endif -%}
    </div>
  </product-info>
  {% unless lazy_load == false or request.design_mode %}</template>{% endunless %}
{%- endif -%}

{% schema %}
{
  "name": "Featured product",
  "tag": "section",
  "class": "section section-featured-product",
  "disabled_on": {
    "groups": ["header", "footer", "aside"],
    "templates": ["password"]
  },
  "blocks": [
    {
      "type": "@app"
    },
    {
      "type": "product_badges",
      "name": "Product badges",
      "settings": [
        {
          "type": "paragraph",
          "content": "These can be customized in [Product badges theme settings](/editor?context=theme&category=Product%2Bbadges)"
        }
      ]
    },
    {
      "type": "text",
      "name": "Text",
      "settings": [
        {
          "type": "inline_richtext",
          "id": "text",
          "default": "Text block",
          "label": "Text"
        },
        {
          "type": "select",
          "id": "text_style",
          "options": [
            {
              "value": "body",
              "label": "Standard"
            },
            {
              "value": "subtitle",
              "label": "Subtitle"
            },
            {
              "value": "uppercase",
              "label": "Uppercase"
            }
          ],
          "default": "body",
          "label": "Text style"
        },
        {
          "type": "select",
          "id": "text_size",
          "options": [
            {
              "value": "t7",
              "label": "3XS"
            },
            {
              "value": "t6",
              "label": "2XS"
            },
            {
              "value": "t5",
              "label": "XS"
            },
            {
              "value": "t4",
              "label": "S"
            },
            {
              "value": "t3",
              "label": "M"
            },
            {
              "value": "t2",
              "label": "L"
            },
            {
              "value": "t1",
              "label": "XL"
            },
            {
              "value": "t0",
              "label": "2XL"
            }
          ],
          "default": "t3",
          "label": "Text size"
        }
      ]
    },
    {
      "type": "richtext",
      "name": "Rich text",
      "settings": [
        {
          "type": "richtext",
          "id": "text",
          "default": "<p>Rich text</p>",
          "label": "Content"
        },
        {
          "type": "select",
          "id": "text_size",
          "options": [
            {
              "value": "t6",
              "label": "2XS"
            },
            {
              "value": "t5",
              "label": "XS"
            },
            {
              "value": "t4",
              "label": "S"
            },
            {
              "value": "t3",
              "label": "M"
            },
            {
              "value": "t2",
              "label": "L"
            },
            {
              "value": "t1",
              "label": "XL"
            }
          ],
          "default": "t3",
          "label": "Text size"
        },
        {
          "type": "header",
          "content": "Visibility"
        },
        {
          "type": "select",
          "id": "stock_visibility",
          "options": [
            {
              "value": "in_stock",
              "label": "Product is in stock"
            },
            {
              "value": "out_of_stock",
              "label": "Product is out of stock"
            },
            {
              "value": "variant_out_of_stock",
              "label": "Selected variant is out of stock"
            },
            {
              "value": "variant_in_stock",
              "label": "Selected variant is in stock"
            },
            {
              "value": "variant_unavailable",
              "label": "Selected variant doesn't exist"
            },
            {
              "value": "either",
              "label": "Show all the time"
            }
          ],
          "info": "Always shows in the Theme Editor.",
          "default": "either",
          "label": "Stock condition to show on"
        }
      ]
    },
    {
      "type": "vendor",
      "limit": 1,
      "name": "Vendor",
      "settings": [
        {
          "type": "select",
          "id": "text_style",
          "options": [
            {
              "value": "body",
              "label": "Standard"
            },
            {
              "value": "subtitle",
              "label": "Subtitle"
            },
            {
              "value": "uppercase",
              "label": "Uppercase"
            }
          ],
          "default": "uppercase",
          "label": "Text style"
        },
        {
          "type": "checkbox",
          "id": "use_link_color",
          "label": "Use link color",
          "default": false
        },
        {
          "type": "select",
          "id": "text_size",
          "options": [
            {
              "value": "t7",
              "label": "3XS"
            },
            {
              "value": "t6",
              "label": "2XS"
            },
            {
              "value": "t5",
              "label": "XS"
            },
            {
              "value": "t4",
              "label": "S"
            },
            {
              "value": "t3",
              "label": "M"
            },
            {
              "value": "t2",
              "label": "L"
            },
            {
              "value": "t1",
              "label": "XL"
            },
            {
              "value": "t0",
              "label": "2XL"
            }
          ],
          "default": "t7",
          "label": "Text size"
        }
      ]
    },
    {
      "type": "title",
      "name": "Title",
      "limit": 1,
      "settings": [
        {
          "type": "select",
          "id": "heading_tag",
          "options": [
            {
              "value": "h1",
              "label": "H1"
            },
            {
              "value": "h2",
              "label": "H2"
            },
            {
              "value": "h3",
              "label": "H3"
            }
          ],
          "default": "h2",
          "info": "For SEO purposes, there should only be one H1 tag on the page.",
          "label": "Heading tag"
        },
        {
          "type": "select",
          "id": "heading_size",
          "options": [
            {
              "value": "h4",
              "label": "XS"
            },
            {
              "value": "h3",
              "label": "S"
            },
            {
              "value": "h2",
              "label": "M"
            },
            {
              "value": "h1",
              "label": "L"
            },
            {
              "value": "h0",
              "label": "XL"
            }
          ],
          "default": "h3",
          "label": "Title size"
        }
      ]
    },
    {
      "type": "price",
      "name": "Price",
      "limit": 1,
      "settings": [
        {
          "type": "checkbox",
          "id": "show_tax_info",
          "label": "Show tax and shipping information",
          "default": true
        },
        {
          "type": "color",
          "id": "price_color",
          "label": "Sale price",
          "info": "Set to transparent to use text color.",
          "default": "#EA0707"
        },
        {
          "type": "select",
          "id": "text_size",
          "options": [
            {
              "value": "h6-size",
              "label": "XS"
