{%- comment -%}theme-check-disable NestedSnippet,SchemaJsonFormat{%- endcomment -%}
{%- liquid
  if section.settings.info_color_scheme != section.settings.section_color_scheme
    assign info_color_scheme = section.settings.info_color_scheme
  endif

  assign vendor_handle = product.vendor | handle
  if collections[vendor_handle].handle != vendor_handle
    assign vendor_url = product.vendor | url_for_vendor
  else
    assign vendor_url = collections[vendor_handle].url
  endif

  assign show_top_title = section.settings.page_title_enabled
  assign zoom_mode = section.settings.image_zoom
  assign product_form_id = 'product-form-' | append: section.id
-%}

<product-info
  id="MainProduct-{{ section.id }}"
  class="section__inner block gradient color-{{ section.settings.section_color_scheme }} {{ section.settings.top_padding }} {{ section.settings.bottom_padding }}"
  data-section="{{ section.id }}"
  data-product-id="{{ product.id }}"
  data-update-url="true"
  data-url="{{ product.url }}"
  data-product-title="{{ product.title | escape }}"
>
  {{ 'section-main-product.css' | asset_url | stylesheet_tag: preload: true }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

  {% unless product.has_only_default_variant %}
    {{ 'component-product-variant-picker.css' | asset_url | stylesheet_tag }}
    {{ 'component-swatch-input.css' | asset_url | stylesheet_tag }}
    {{ 'component-swatch.css' | asset_url | stylesheet_tag }}
  {% endunless %}

  {%- if product.quantity_price_breaks_configured? -%}
    <link rel="stylesheet" href="{{ 'component-volume-pricing.css' | asset_url }}" media="print" onload="this.media='all'">
  {%- endif -%}

  <lazy-script data-watch-element=".product,.sticky-cta" src="{{ 'product-info.js' | asset_url }}" defer="defer"></lazy-script>

  {%- unless settings.card_enable_quick_add -%}
    <lazy-script data-watch-element=".product,.sticky-cta" src="{{ 'product-form.js' | asset_url }}" defer="defer"></lazy-script>
  {%- endunless -%}

  {%- if product.quantity_price_breaks_configured? -%}
    <lazy-script data-watch-element=".product,.sticky-cta" src="{{ 'show-more.js' | asset_url }}" defer="defer"></lazy-script>
    <lazy-script data-watch-element=".product,.sticky-cta" src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></lazy-script>
  {%- endif -%}

  {% if zoom_mode == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
  <div class="page-width">
    {%- if show_top_title -%}
      <div class="max-w-m xs:pl-mobile xs:pr-mobile pb-s075 quick-add-hidden {{ section.settings.page_title_visibility }}
          {% if section.settings.top_padding == 'pt-0' %}pt-s075{% elsif section.settings.top_padding == 'pt-s05' %}pt-s05{% else %}pt-0{% endif %}"
      >
        {%- if section.settings.page_title_vendor -%}
          <p class="t7 mt-0 uppercase caption-with-letter-spacing">
            {{ product.vendor | link_to: vendor_url, class: 'un-underlined-link text-current' }}
          </p>
        {%- endif -%}

        <h1 class="b-main-title {{ section.settings.page_title_size }} text-balance m-0" data-product-title-id="{{ product.id }}">
          {{ product.title | escape }}
        </h1>

        {%- if section.settings.page_title_review_stars and product.metafields.reviews.rating.value != blank -%}
          {% liquid
            if section.settings.page_title_size == 'h4'
              assign rating_size = 2.3
            elsif section.settings.page_title_size == 'h3'
              assign rating_size = 2.7
            elsif section.settings.page_title_size == 'h2'
              assign rating_size = 2.9
            elsif section.settings.page_title_size == 'h1'
              assign rating_size = 3.2
            elsif section.settings.page_title_size == 'h0'
              assign rating_size = 3.5
            endif

            assign rating_decimal = 0
            assign decimal = product.metafields.reviews.rating.value.rating | modulo: 1
            if decimal >= 0.3 and decimal <= 0.7
              assign rating_decimal = 0.5
            elsif decimal > 0.7
              assign rating_decimal = 1
            endif
          %}
          <div class="mt-05">
            <div
              class="rating"
              role="img"
              aria-label="{{ 'accessibility.star_reviews_info' | t: rating_value: product.metafields.reviews.rating.value, rating_max: product.metafields.reviews.rating.value.scale_max }}"
            >
              <span
                aria-hidden="true"
                class="rating-star"
                style="--rating: {{ product.metafields.reviews.rating.value.rating | floor }}; --rating-max: {{ product.metafields.reviews.rating.value.scale_max }}; --rating-decimal: {{ rating_decimal }}; --font-size: {{ rating_size }}; --color-rating-star: rgba({{ section.settings.star_color.red }},{{ section.settings.star_color.green }},{{ section.settings.star_color.blue }});"
              ></span>
            </div>
          </div>
        {%- endif -%}

        {%- if section.settings.page_title_price and product.price > 0 -%}
          {%- liquid
            assign money_price = product.price | money
            if settings.currency_code_enabled
              assign money_price = product.price | money_with_currency
            endif

            if product.price_varies
              assign money_price = 'products.product.price.from_price_html' | t: price: money_price
            endif

            if section.settings.page_title_size == 'h4'
              assign price_size = 't2'
            elsif section.settings.page_title_size == 'h3'
              assign price_size = 't2'
            elsif section.settings.page_title_size == 'h2'
              assign price_size = 't1'
            elsif section.settings.page_title_size == 'h1'
              assign price_size = 't0'
            elsif section.settings.page_title_size == 'h0'
              assign price_size = 't0'
            endif
          -%}
          <p class="{{ price_size }} {% unless section.settings.page_title_review_stars and product.metafields.reviews.rating.value != blank %}mt-1{% else %}mt-05{% endunless %} mb-0">{{ money_price }}</p>
        {%- endif -%}
      </div>
    {%- endif -%}

    {%- capture panels_below_media -%}
      {%- assign is_in_panel_below_media = false -%}
      {%- for block in section.blocks -%}
        {%- liquid
          assign is_hidden = false
          if block.settings.stock_visibility == 'in_stock' and product.available != true
            assign is_hidden = true
          elsif block.settings.stock_visibility == 'out_of_stock' and product.available
            assign is_hidden = true
          endif
        -%}

        {%- if block.type == 'panel' -%}
          {%- liquid
            if block.settings.color_scheme != section.settings.section_color_scheme
              assign panel_color_scheme = block.settings.color_scheme
            else
              assign panel_color_scheme = false
            endif

            assign is_in_panel = true

            if block.settings.below_media
              assign is_in_panel_below_media = true
            else
              assign is_in_panel_below_media = false
            endif
          -%}

          {%- if is_in_panel_below_media -%}
            </div>
            <div class="product__info-container product__info-panel{% if is_hidden == false %} {{ block.settings.visibility }}{% if block.settings.stock_visibility contains 'variant' and request.design_mode == false %} visible-{{ block.settings.stock_visibility }}{% endif %}{% if block.settings.show_at_specific_time_of_day and request.design_mode == false %}{% if block.settings.show_time_morning == false %} hidden-morning {% endif %}{% if block.settings.show_time_afternoon == false %} hidden-afternoon {% endif %}{% if block.settings.show_time_evening == false %} hidden-evening {% endif %}{% if block.settings.show_time_night == false %} hidden-night {% endif %}{% endif %}{% endif %}{% if block.settings.show_in_quickbuy == false %} quick-add-hidden{% endif %}{% if panel_color_scheme %} gradient color-{{ panel_color_scheme }} product__info-padded border-radius-text-box {% if section.settings.card_shadow %} card--shadow{% endif %}{% endif %}"
              {% if is_hidden and request.design_mode != true %} hidden{% endif %}
              {% if block.settings.padding > 0 %}style="--info-padding-x:{{ block.settings.padding }}rem;--info-padding-y:{{ block.settings.padding }}rem;"{% endif %}
            >
              {%- if block.settings.panel_heading != blank or block.settings.panel_icon != blank -%}
                <{{ block.settings.heading_tag }} class="panel__label flex items-center gap-1 m-0 {{ block.settings.heading_style }} {{ block.settings.heading_text_size }} font-{{ block.settings.heading_text_weight }}">
                  {%- if block.settings.panel_icon != blank -%}
                    {%- render 'icon', icon: block.settings.panel_icon -%}
                  {%- endif -%}
                  {{ block.settings.panel_heading }}
                </{{ block.settings.heading_tag }}>
              {%- endif -%}
          {%- endif -%}

        {%- elsif is_in_panel_below_media and is_hidden == false -%}
          {%- render 'product-block',
              section: section,
              block: block,
              product: product,
              is_featured: false,
              product_form_id: product_form_id,
              is_in_panel: is_in_panel,
              vendor_url: vendor_url,
              panel_color_scheme: panel_color_scheme,
              info_color_scheme: info_color_scheme
          -%}
        {%- endif -%}
      {%- endfor -%}
    {%- endcapture -%}

    {%- liquid
        if section.settings.gallery_layout == 'slider' and section.settings.enable_sticky_info and panels_below_media == blank
          assign enable_sticky_info = true
        endif
    -%}

    <div class="product grid bg-scroll grid--1-col
      product--{{ section.settings.media_size }}
      product--left
      product--slider
      {% if section.settings.stack_thumbnails and section.settings.gallery_layout == 'slider' %}
        product--thumbnail
      {% else %}
        product--thumbnail_slider
      {% endif %}
      {% if section.settings.gallery_layout != 'slider' %}
        product--{{ section.settings.gallery_layout }}
      {% else %}
        product--slider-desktop
      {% endif %}
      {% if panels_below_media != blank %}
        product--three-grid
      {% endif %}
      product--mobile-{% if section.settings.thumbnail_layout == 'both' or section.settings.thumbnail_layout == 'mobile' %}show{% else %}hide{% endif %}
      {% if product.media.size > 0 %}grid--2-col-desktop{% else %}product--no-media{% endif %}"
      data-product-handle="{{ product.handle }}"
      {% if product.vendor %}data-product-vendor="{{ product.vendor | handleize }}"{% endif %}
      data-variant-status="{% if product.selected_or_first_available_variant == nil %}unavailable{% elsif product.selected_or_first_available_variant.available %}in-stock{% elsif product.selected_or_first_available_variant %}out-of-stock{% endif %}"
    >

      <div class="grid__item product__media-wrapper">
        {% render 'product-media-gallery', variant_images: variant_images, gallery_layout: section.settings.gallery_layout, enable_sticky_info: enable_sticky_info %}
      </div>

      <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}">
        <section
          id="ProductInfo-{{ section.id }}"
          class="product__info-container block{% if enable_sticky_info %} product__column-sticky{% endif %}"
        >
          <div class="product__info-container {% if info_color_scheme %}gradient color-{{ info_color_scheme }} product__info-padded product__info-padded-main border-radius-text-box{% if section.settings.card_shadow %} card--shadow{% endif %}{% if section.settings.info_fullwidth_mob %} info-fullwidth-mob{% endif %}{% endif %}">
            {%- assign is_in_panel_below_media = false -%}
            {%- for block in section.blocks -%}
              {%- liquid
                assign is_hidden = false
                if block.settings.stock_visibility == 'in_stock' and product.available != true
                  assign is_hidden = true
                elsif block.settings.stock_visibility == 'out_of_stock' and product.available
                  assign is_hidden = true
                endif
              -%}

              {%- if is_in_panel_below_media == false or block.type == 'panel' -%}
                {%- if block.type == 'panel' -%}
                  {%- liquid
                    if block.settings.color_scheme != section.settings.section_color_scheme
                      assign panel_color_scheme = block.settings.color_scheme
                    else
                      assign panel_color_scheme = false
                    endif

                    assign is_in_panel = true

                    if block.settings.below_media
                      assign is_in_panel_below_media = true
                    else
                      assign is_in_panel_below_media = false
                    endif
                  -%}

                  {%- if is_in_panel_below_media == false -%}
                    </div>
                    <div class="product__info-container product__info-panel{% if is_hidden == false %} {{ block.settings.visibility }}{% if block.settings.stock_visibility contains 'variant' and request.design_mode == false %} visible-{{ block.settings.stock_visibility }}{% endif %}{% if block.settings.show_at_specific_time_of_day and request.design_mode == false %}{% if block.settings.show_time_morning == false %} hidden-morning {% endif %}{% if block.settings.show_time_afternoon == false %} hidden-afternoon {% endif %}{% if block.settings.show_time_evening == false %} hidden-evening {% endif %}{% if block.settings.show_time_night == false %} hidden-night {% endif %}{% endif %}{% endif %}{% if block.settings.show_in_quickbuy == false %} quick-add-hidden{% endif %}{% if panel_color_scheme %} gradient color-{{ panel_color_scheme }} product__info-padded border-radius-text-box{% if section.settings.card_shadow %} card--shadow{% endif %}{% endif %}"
                      {% if is_hidden and request.design_mode != true %} hidden{% endif %}
                      {% if block.settings.padding > 0 %}style="--info-padding-x:{{ block.settings.padding }}rem;--info-padding-y:{{ block.settings.padding }}rem;"{% endif %}
                    >
                      {%- if block.settings.panel_heading != blank or block.settings.panel_icon != blank -%}
                        <{{ block.settings.heading_tag }} class="panel__label flex items-center gap-1 m-0 {{ block.settings.heading_style }} {{ block.settings.heading_text_size }} font-{{ block.settings.heading_text_weight }}">
                          {%- if block.settings.panel_icon != blank -%}
                            {%- render 'icon', icon: block.settings.panel_icon -%}
                          {%- endif -%}
                          {{ block.settings.panel_heading }}
                        </{{ block.settings.heading_tag }}>
                      {%- endif -%}
                  {%- endif -%}

                {%- elsif is_hidden == false -%}
                  {%- render 'product-block',
                      section: section,
                      block: block,
                      product: product,
                      is_featured: false,
                      product_form_id: product_form_id,
                      is_in_panel: is_in_panel,
                      vendor_url: vendor_url,
                      panel_color_scheme: panel_color_scheme,
                      info_color_scheme: info_color_scheme
                  -%}
                {%- endif -%}

              {%- endif -%}
            {%- endfor -%}
          </div>
        </section>
      </div>

      {%- if panels_below_media != blank -%}
        <section
            id="ProductMediaInfo-{{ section.id }}"
            class="product__media-submedia-wrapper product__info-container block{% if enable_sticky_info %} product__column-sticky{% endif %}"
        >
          <div>{{- panels_below_media -}}</div>
        </section>
      {%- endif -%}
    </div>

    {%- if zoom_mode and zoom_mode != 'none' -%}
      {% render 'product-media-modal' variant_images: variant_images %}
    {%- endif -%}

    {%- assign popups = section.blocks | where: 'type', 'popup' -%}
    {%- if popups.size > 0 -%}
      <link rel="stylesheet" href="{{ 'component-popup.css' | asset_url }}" media="print" onload="this.media='all'">
      <link rel="stylesheet" href="{{ 'component-popup-page.css' | asset_url }}" media="print" onload="this.media='all'">
    {%- endif -%}
    {%- for block in popups -%}
      <modal-dialog id="PopupModal-{{ block.id }}" class="popup-modal popup-modal--small{% if block.settings.popup_style == 'side' %} popup-modal--side{% endif %}" {{ block.shopify_attributes }}>
        <template data-prevent-interpolation class="deferred">
        <div
          role="dialog"
          aria-label="{{ block.settings.text | escape }}"
          aria-modal="true"
          class="popup-modal__content min-w-0 custom-scrollbar"
          tabindex="-1"
          style="width: min({{ block.settings.side_popup_max_width }}px, 90vw)"
        >
          <button
            id="ModalClose-{{ block.id }}"
            type="button"
            class="popup-modal__toggle"
            aria-label="{{ 'accessibility.close' | t }}"
          >
            {% render 'icon-close' %}
          </button>
          <div class="popup-modal__content-info rte">
            {{ block.settings.content }}
            {{ block.settings.page.content }}
          </div>
        </div>
        </template>
      </modal-dialog>
    {%- endfor -%}

    {%- assign variant_picker_block = section.blocks | where: 'type', 'variant_picker' | first -%}
    {%- if variant_picker_block and variant_picker_block.settings.size_chart_page.content != blank -%}
      <link rel="stylesheet" href="{{ 'component-popup.css' | asset_url }}" media="print" onload="this.media='all'">
      <link rel="stylesheet" href="{{ 'component-popup-page.css' | asset_url }}" media="print" onload="this.media='all'">
      <modal-dialog id="PopupModal-{{ variant_picker_block.id }}" class="popup-modal popup-modal--small{% if variant_picker_block.settings.popup_style == 'side' %} popup-modal--side{% endif %}" {{ variant_picker_block.shopify_attributes }}>
        <template data-prevent-interpolation class="deferred">
        <div
            role="dialog"
            aria-label="{{ variant_picker_block.settings.size_chart_label | escape }}"
            aria-modal="true"
            class="popup-modal__content custom-scrollbar"
            tabindex="-1"
        >
          <button
              id="ModalClose-{{ variant_picker_block.id }}"
              type="button"
              class="popup-modal__toggle"
              aria-label="{{ 'accessibility.close' | t }}"
          >
            {% render 'icon-close' %}
          </button>
          <div class="popup-modal__content-info rte">
            {{ variant_picker_block.settings.size_chart_page.content }}
          </div>
        </div>
        </template>
      </modal-dialog>
    {%- endif -%}

    {%- if product.media.size > 0 -%}
      <lazy-script data-watch-element=".product,.sticky-cta" src="{{ 'product-modal.js' | asset_url }}" defer="defer"></lazy-script>
      <lazy-script data-watch-element=".product,.sticky-cta" src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></lazy-script>
    {%- endif -%}

    {%- if first_3d_model -%}
      <script type="application/json" id="ProductJSON-{{ product.id }}">
        {{ product.media | where: 'media_type', 'model' | json }}
      </script>
      <script src="{{ 'product-model.js' | asset_url }}" defer></script>
    {%- endif -%}

    {%- if settings.seo_schema_product != 'none' -%}
      {%- if settings.seo_schema_product == 'shopify' -%}
        <script type="application/ld+json">
          {{ product | structured_data }}
        </script>
      {%- else -%}
        {%- liquid
          if product.selected_or_first_available_variant.featured_media
            assign seo_media = product.selected_or_first_available_variant.featured_media
          else
            assign seo_media = product.featured_media
          endif
        -%}

        <script type="application/ld+json">
          {
            "@context": "https://schema.org/",
            "@type": "Product",
            "name": {{ product.title | json }},
            "url": {{ request.origin | append: product.url | json }},
            {% if seo_media -%}
              "image": [
                {{ seo_media | image_url: width: 1920 | prepend: "https:" | json }}
              ],
            {%- endif %}
            "description": {{ product.description | strip_html | json }},
            {% if product.selected_or_first_available_variant.sku != blank -%}
              "sku": {{ product.selected_or_first_available_variant.sku | json }},
            {%- endif %}
            "brand": {
              "@type": "Brand",
              "name": {{ product.vendor | json }}
            },
            {% if product.metafields.reviews.rating.value != blank %}
            "aggregateRating": {
              "@type": "AggregateRating",
              "ratingValue": {{ product.metafields.reviews.rating.value }},
              "reviewCount": {{ product.metafields.reviews.rating_count }}
            },
            {% endif %}
            "offers": [
              {%- for variant in product.variants -%}
                {
                  "@type" : "Offer",
                  {%- if variant.sku != blank -%}
                    "sku": {{ variant.sku | json }},
                  {%- endif -%}
                  {%- if variant.barcode.size == 12 -%}
                    "gtin12": {{ variant.barcode | json }},
                  {%- endif -%}
                  {%- if variant.barcode.size == 13 -%}
                    "gtin13": {{ variant.barcode | json }},
                  {%- endif -%}
                  {%- if variant.barcode.size == 14 -%}
                    "gtin14": {{ variant.barcode | json }},
                  {%- endif -%}
                  "itemCondition" : "https://schema.org/NewCondition",
                  "priceValidUntil": "{{ 'now' | date: '%s' | plus: 31536000 | date: '%Y-%m-%d' | url_encode | replace: '+', '%20' }}",
                  "availability" : "https://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
                  "price" : {{ variant.price | divided_by: 100.00 | json }},
                  "priceCurrency" : {{ cart.currency.iso_code | json }},
                  "url" : {{ request.origin | append: variant.url | json }},
                  "seller": {
                    "@type": "Organization",
                    "name": {{ shop.name | json }},
                    {% if section.settings.logo %}
                      "logo": {{ section.settings.logo | image_url: width: 500 | prepend: "https:" | json }},
                    {% endif %}
                    "url": {{ request.origin | append: page.url | json }}
                  }
                }{% unless forloop.last %},{% endunless %}
              {%- endfor -%}
            ]
          }
        </script>
      {%- endif -%}
    {%- endif -%}
  </div>

  {%- assign buy_buttons_block = section.blocks | where: 'type', 'buy_buttons' -%}
  {%- if section.settings.sticky_panel != "off" and product.available and buy_buttons_block.size > 0 -%}
    {%- liquid
      assign price = product.selected_or_first_available_variant.price
      if settings.currency_code_enabled
        assign money_price = price | money_with_currency
      else
        assign money_price = price | money
      endif

      assign check_against_inventory = true
      if product.selected_or_first_available_variant.inventory_management != 'shopify' or product.selected_or_first_available_variant.inventory_policy == 'continue'
        assign check_against_inventory = false
      endif
      if product.selected_or_first_available_variant.quantity_rule.min > product.selected_or_first_available_variant.inventory_quantity and check_against_inventory
        assign quantity_rule_soldout = true
      endif

      assign buy_buttons_block = buy_buttons_block | first
    -%}

    {{ 'component-sticky-cta.css' | asset_url | stylesheet_tag }}
    <div id="sticky-cta-{{ section.id }}" class="sticky-cta hidden-during-load quick-add-hidden color-{{ section.settings.cta_color_scheme }} gradient card--shadow p-1 flex items-center border-radius-panel gap-3 {{ section.settings.sticky_panel }}">
      <div class="sticky-cta__info pl-1 lh-15">
        <strong class="t3">{{ product.title }}</strong>
        <p class="t5 mt-025 mb-0 sticky-cta__price">{{ money_price }}</p>
      </div>
      <div class="sticky-cta__button pr-1">
        <button data-anchor="#MainProduct-{{ section.id }}" class="button button--primary button--small button--full-width
          {% if buy_buttons_block.settings.button_text_font_weight == 'bold' %} font-bold{% endif %}
          {% if buy_buttons_block.settings.icon != blank %} button-with-icon{% endif %}
          {% if buy_buttons_block.settings.icon != blank and buy_buttons_block.settings.icon_position == 'right' %} button--full-width-icon-right{% endif %}"
            {% if product.selected_or_first_available_variant.available == false
              or quantity_rule_soldout
              or product.selected_or_first_available_variant == nil
            %}
            disabled
          {% endif %}
          style="--font-button-case: {{ buy_buttons_block.settings.button_text_transform }};--font-button-font-weight: {{ buy_buttons_block.settings.button_text_font_weight }};"
        >
          {%- if buy_buttons_block.settings.icon != blank and buy_buttons_block.settings.icon_position == 'left' -%}
            {%- render 'icon', icon: buy_buttons_block.settings.icon, class: 'relative' -%}
          {%- endif -%}
          <span>
            {%- if product.selected_or_first_available_variant == nil -%}
              {{ 'products.product.unavailable' | t }}
            {%- elsif product.selected_or_first_available_variant.available == false or quantity_rule_soldout -%}
              {{ 'products.product.sold_out' | t }}
            {%- elsif product.template_suffix contains 'preorder' -%}
              {{ 'products.product.preorder' | t }}
            {%- else -%}
              {{ 'products.product.add_to_cart' | t }}
            {%- endif -%}
          </span>
          {%- if buy_buttons_block.settings.icon != blank and buy_buttons_block.settings.icon_position == 'right' -%}
            {%- render 'icon', icon: buy_buttons_block.settings.icon, class: 'relative' -%}
          {%- endif -%}

          {%- render 'loading-spinner' -%}
        </button>
      </div>
    </div>
  {%- endif -%}
</product-info>

{%- if request.design_mode -%}
  {%- liquid
    assign table_blocks = section.blocks | where: 'type', 'table'
    assign show_alert = false
    for block in table_blocks
      if block.settings.label_1 != empty
        unless block.settings.label_1 contains '<p>' or block.settings.label_1 contains '<ul>' or block.settings.label_1 contains '<ol>' or block.settings.label_1 contains '<h1>' or block.settings.label_1 contains '<h2>' or block.settings.label_1 contains '<h3>' or block.settings.label_1 contains '<h4>' or block.settings.label_1 contains '<h5>' or block.settings.label_1 contains '<h6>'
          assign show_alert = true
        endunless
      endif
    endfor
  -%}
  {%- if show_alert -%}
    <section class="section-popup no-move">
      <link rel="stylesheet" href="{{ 'component-popup.css' | asset_url }}" media="print" onload="this.media='all'">
      <link rel="stylesheet" href="{{ 'component-popup-page.css' | asset_url }}" media="print" onload="this.media='all'">

      <custom-popup id="PopupModal-table-upgrade" class="popup-modal p-2 flex justify-center items-center md:justify-start md:items-end" style="--responsive-card-padding: 3.1rem;"
        data-popup-id="table-upgrade" data-mode="enabled" data-te-only="true"
      >
        <div role="dialog" aria-modal="true" class="popup-modal__content custom-scrollbar color-scheme-2 gradient">
          <div class="popup__message-container relative image-mobile-top image-desktop-left">
            <div class="popup__image media" style="--image-size-mobile: 230px;--image-size-desktop: 230px;"></div>
            <div class="w-full section-content responsive-card-padding card-padding max-w-s text-left md:text-left">
              <h2 class="popup__heading inline-richtext section-content__heading color-heading h4 self-left text-left md:self-left md:text-left">
                <strong>Important</strong>
              </h2>
              <div class="popup__text section-content__text rte max-w- t4 mb-0">
                <p>
                  Thank you for updating Ignite. To complete the update you will need to make a small change (e.g. adding a full stop) in every
                  <strong>Table</strong> block in the <strong>Product information</strong> section on this page and then
                  click <strong>Save</strong>.
                </p>
                <p>
                  You may see the error below in the Theme Editor if you don't do this.
                  <img src="https://cdn.shopify.com/s/files/1/0672/3731/4782/files/Screenshot-2025-03-27-at-20.31.03.png?v=1743110266" style="border-radius: 0" class="w-full"/>
                </p>
              </div>

              <div class="popup__buttons section-content__buttons justify-left md:justify-left font-bold">
                <a href="https://benchmarkthemes.com/help-center/ignite/guides/updating-your-theme/#:~:text=within%20your%20dashboard.-,Upgrading%20from%20v1.3.0,-If%20you%E2%80%99ve%20just" data-modal-close class="t2 link un-underlined-link button-with-icon button-with-icon--right">
                  Read more/Watch video
                  {% render 'icon-arrow' %}
                </a>
                <hr class="m-0"/>
                <p>
                  <button id="ModalClose-" type="button" class="button button--secondary">
                    I understand, don't show again.
                  </button>
                </p>
              </div>
            </div>
          </div>
        </div>
      </custom-popup>
    </section>
  {%- endif -%}
{%- endif -%}

